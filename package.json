{
  "name": "ai-assisted-testing",
  "version": "1.0.0",
  "description": "AI-assisted testing framework with data generation and NLP test case creation",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node",
    "gen:data": "ts-node scripts/generate-testdata.ts",
    "gen:testcases": "ts-node scripts/generate-testcases.ts",
    "test:e2e": "playwright test --config=apps/e2e/playwright.config.ts",
    "test:e2e:ui": "playwright test --config=apps/e2e/playwright.config.ts --ui",
    "test:e2e:report": "playwright show-report apps/e2e/playwright-report",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "keywords": ["testing", "ai", "playwright", "nlp", "test-generation"],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@types/node": "^24.3.0",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "zod": "^4.1.5"
  },
  "dependencies": {
    "dotenv": "^17.2.1"
  }
}
